<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/some_id"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clipToOutline="true"
    android:background="#F4F4F4"
    >
    <View
        android:id="@+id/h_underbar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="630dp"
        android:background="@drawable/h_img9" />
    <RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/h_homeicon"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="20dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="655dp"
        android:clipToOutline="true"
        android:background="@drawable/h_img7"
        />
    <RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/h_timetable"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="130dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="655dp"
        android:clipToOutline="true"
        android:background="@drawable/h_img4"
        />
    <RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/h_chatting"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="240dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="655dp"
        android:clipToOutline="true"
        android:background="@drawable/h_img8"
        />
    <RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/user_02"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="340dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="655dp"
        android:clipToOutline="true"
        android:background="@drawable/h_img6"
        />
    <View
        android:id="@+id/h_rectangle"
        android:layout_width="297dp"
        android:layout_height="200dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="55dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="280dp"
        android:background="@drawable/h_img14"
        />
    <View
        android:id="@+id/h_rectangle2"
        android:layout_width="294dp"
        android:layout_height="54dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="55dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="530dp"
        android:background="@drawable/h_img15"
        android:elevation="4dp"
        />
    <View
        android:id="@+id/h_rectangle3"
        android:layout_width="290dp"
        android:layout_height="45dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="55dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="165dp"
        android:background="@drawable/h_img16"
        />
    <RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/chevron"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="20dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="30dp"
        android:background="@drawable/h_img17"
        />

    <TextView
        android:id="@+id/h_inbutton"
        android:layout_width="117dp"
        android:layout_height="54dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="164dp"
        android:layout_marginTop="155dp"
        android:gravity="center_vertical"
        android:lineSpacingExtra="6sp"
        android:text="@string/h_inbutton"
        android:textAppearance="@style/h_inbutton" />
    <TextView
        android:id="@+id/goal"
        android:layout_width="79dp"
        android:layout_height="19dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="56dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="250dp"
        android:text="@string/h_goal"
        android:textAppearance="@style/h_goal"
        android:lineSpacingExtra="2sp"
        android:gravity="center_vertical"
        />


</RelativeLayout>